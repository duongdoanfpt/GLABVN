@model IEnumerable<PS11905_BAODUONG_ASMFULL_NET104.Models.NhomSp>

@{
    
    Layout = "~/Views/Shared/_LayoutCpanel.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        <a asp-action="Create">Create New</a>
